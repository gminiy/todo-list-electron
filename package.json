{
  "name": "nodejs-todo",
  "version": "1.0.0",
  "description": "todo application",
  "main": "./main.js",
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . todo --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/win/iconWin.icns --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Todo App\"",
    "package-linux": "electron-packager . todo --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gminiy/nodejs-todo.git"
  },
  "author": "ingleby",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.4",
    "electron-packager": "^13.1.1"
  }
}
